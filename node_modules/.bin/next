#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/blaze/mine/commitdiary/node_modules/.pnpm/next@13.4.12_react-dom@18.2.0_react@18.2.0__react@18.2.0_sass@1.94.2/node_modules/next/dist/bin/node_modules:/home/blaze/mine/commitdiary/node_modules/.pnpm/next@13.4.12_react-dom@18.2.0_react@18.2.0__react@18.2.0_sass@1.94.2/node_modules/next/dist/node_modules:/home/blaze/mine/commitdiary/node_modules/.pnpm/next@13.4.12_react-dom@18.2.0_react@18.2.0__react@18.2.0_sass@1.94.2/node_modules/next/node_modules:/home/blaze/mine/commitdiary/node_modules/.pnpm/next@13.4.12_react-dom@18.2.0_react@18.2.0__react@18.2.0_sass@1.94.2/node_modules:/home/blaze/mine/commitdiary/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/blaze/mine/commitdiary/node_modules/.pnpm/next@13.4.12_react-dom@18.2.0_react@18.2.0__react@18.2.0_sass@1.94.2/node_modules/next/dist/bin/node_modules:/home/blaze/mine/commitdiary/node_modules/.pnpm/next@13.4.12_react-dom@18.2.0_react@18.2.0__react@18.2.0_sass@1.94.2/node_modules/next/dist/node_modules:/home/blaze/mine/commitdiary/node_modules/.pnpm/next@13.4.12_react-dom@18.2.0_react@18.2.0__react@18.2.0_sass@1.94.2/node_modules/next/node_modules:/home/blaze/mine/commitdiary/node_modules/.pnpm/next@13.4.12_react-dom@18.2.0_react@18.2.0__react@18.2.0_sass@1.94.2/node_modules:/home/blaze/mine/commitdiary/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../next/dist/bin/next" "$@"
else
  exec node  "$basedir/../next/dist/bin/next" "$@"
fi
